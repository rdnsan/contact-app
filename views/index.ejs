<h1 class="text-center mt-5 text-secondary">Express Contact App</h1>
<h4 class="text-end">Hello, <%= name %></h4>
<hr />
<div class="row mt-4">
  <div class="col-8 mx-auto">
    <h3 class="text-secondary">Maintainer</h3>
    <table class="table table-hover table-bordered border-info">
      <thead>
        <tr class="table-info border-info">
          <th scope="col">#</th>
          <th scope="col">Nama</th>
          <th scope="col">Email</th>
        </tr>
      </thead>
      <tbody>
        <% maintainers.forEach((maintainer, index) => { %>
        <tr>
          <th scope="row"><%= index + 1 %></th>
          <td><%= maintainer.name %></td>
          <td><%= maintainer.email %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</div>
